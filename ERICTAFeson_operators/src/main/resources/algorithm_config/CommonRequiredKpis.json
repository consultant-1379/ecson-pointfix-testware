      "object_type": "FLOAT",
      "calculation_frequency": "0 0 1 * * ? *",
      "aggregation_type": "SUM",
      "aggregation_period": 1440,
      "filter": [
        "kpi_db://kpi_relation_guid_source_guid_target_guid_1440.local_timestamp = TO_TIMESTAMP(1593993600)",
        "kpi_db://kpi_relation_guid_source_guid_target_guid_1440.intra_site = false OR intra_site IS NULL"
      ],
      "aggregation_elements": [
        "kpi_relation_guid_source_guid_target_guid_1440.source_guid as guid"
