      "aggregation_type": "FIRST",
      "aggregation_period": 1440,
      "aggregation_elements": [
        "kpi_cell_guid_1440.guid"
      ],
      "filter": [
        "kpi_db://kpi_cell_guid_1440.local_timestamp = TO_TIMESTAMP(DATE_SUB(CURRENT_DATE(),1))"
      ],
      "is_visible": false
    }
