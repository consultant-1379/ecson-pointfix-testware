    {
      "name": "rrc_congestion_rate_numerator",
      "alias": "cell_guid",
      "expression": "100 * (counters_cell.pmRrcConnEstabFailMpOverload + counters_cell.pmRrcConnEstabFailDuIntens + counters_cell.pmRrcConnEstabFailCellIntensDLC + counters_cell.pmRrcConnEstabFailCellIntensStat + counters_cell.pmRrcConnEstabFailCellLatency) FROM pm_stats://counters_cell",
