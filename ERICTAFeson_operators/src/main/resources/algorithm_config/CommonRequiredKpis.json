      "name": "rrc_congestion_rate_neigh",
      "alias": "cell_guid",
      "expression": "(cell_guid.rrc_congestion_rate * relation_guid_source_guid_target_guid.out_succ_ho_hourly) / cell_guid.cell_succ_ho_hourly FROM kpi://cell_guid LEFT JOIN kpi://relation_guid_source_guid_target_guid ON cell_guid.guid = relation_guid_source_guid_target_guid.source_guid AND cell_guid.local_timestamp = relation_guid_source_guid_target_guid.local_timestamp",
